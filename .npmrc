# Оптимизация для Railway
prefer-offline=true
audit=false
fund=false
progress=false
loglevel=warn

# Кэширование
package-lock=true

# Оптимизация установки зависимостей
legacy-peer-deps=false
strict-peer-deps=false

# Note: We allow Puppeteer to download bundled Chromium as a fallback
# System Chromium from nixpkgs will be tried first