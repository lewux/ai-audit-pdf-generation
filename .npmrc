# Оптимизация для Railway (Dockerfile build)
prefer-offline=true
audit=false
fund=false
progress=false
loglevel=warn

# Кэширование
package-lock=true

# Оптимизация установки зависимостей
legacy-peer-deps=false
strict-peer-deps=false

# Skip Puppeteer Chromium download - using system Chromium from Dockerfile
puppeteer_skip_chromium_download=true