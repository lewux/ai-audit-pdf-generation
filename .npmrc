# Оптимизация для Railway
prefer-offline=true
audit=false
fund=false
progress=false
loglevel=warn

# Кэширование
package-lock=true

# Оптимизация установки зависимостей
legacy-peer-deps=false
strict-peer-deps=false

# Puppeteer - skip bundled Chromium download (using system chromium from nixpkgs)
puppeteer_skip_chromium_download=true